{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Apps72.Dev.Data.dll"
          ],
          "src": "../src/Core/bin/Release/netstandard2.0"
        }
      ],
      "dest": "api/dbcmd",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/dbcmd/**.yml",
          "toc.yml",
          "api/toc.yml",
          "api/index.md",
          "api/dbcmd/index.md"
        ]
      },
      {
        "files": [
          "dbcmd/**.md",
          "dbcmd/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "dbmocker/**.md",
          "dbmocker/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Apps72.Dev.Data",
      "_appFooter": "Developed By Denis Voituron",
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/favicon.ico",
      "_disableBreadcrumb": false,
      "_disableAffix": false,
      "_disableContribution": false
    },
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/material"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}